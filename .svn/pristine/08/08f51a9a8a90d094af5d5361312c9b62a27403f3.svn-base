    </el-menu>*@



    @RenderBody()
    @RenderSection("scripts", required: false)
</body>
</html>
<script>
    //if (top.location != self.location)
    //    top.location = self.location;

    L2Dwidget.init({
        "display": {
            "superSample": 2,
            "width": 100,
            "height": 200,
            "position": "left",
            "hOffset": 0,
            "vOffset": 0
        }
    });
</script>